{
  "name": "retently-test-task",
  "version": "1.0.0",
  "description": "Test Task for Retently",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --config jest.config.js --setupFiles dotenv-safe/config --watchAll",
    "format": "prettier --config .prettierrc.json --write \"{src,tests}/**/*.{ts,json,js}\"",
    "start": "ts-node -T -r dotenv-safe/config src",
    "start:debug": "node --inspect-brk -r ts-node/register -r dotenv-safe/config src/index.ts"
  },
  "author": "Arsenii Dumitru",
  "repository": {
    "url": "https://github.com/arsdu24/retently-test-task.git",
    "type": "git"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "sharp": "^0.24.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/busboy": "^0.2.3",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.0",
    "@types/jsonwebtoken": "^8.3.6",
    "@types/node": "^13.1.8",
    "@types/sharp": "^0.24.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  }
}
